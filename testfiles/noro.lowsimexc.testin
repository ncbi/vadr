out: va-noro.lowsimexc.1/va-noro.lowsimexc.1.vadr.fail.tbl
out: va-noro.lowsimexc.1/va-noro.lowsimexc.1.vadr.alt
exp: @VADRSCRIPTSDIR@/testfiles/expected-files/va-noro.lowsimexc.1/va-noro.lowsimexc.1.vadr.pass.tbl 
exp: @VADRSCRIPTSDIR@/testfiles/expected-files/va-noro.lowsimexc.1/va-noro.lowsimexc.1.vadr.fail.tbl 
exp: @VADRSCRIPTSDIR@/testfiles/expected-files/va-noro.lowsimexc.1/va-noro.lowsimexc.1.vadr.alt
rmdir: va-noro.lowsimexc.1
