exp: @VADRSCRIPTSDIR@/testfiles/expected-files/va-entoy100a-extrant2/va-entoy100a-extrant2.vadr.alt
rmdir: va-entoy100a-extrant2
#
command: perl $VADRSCRIPTSDIR/v-annotate.pl -f --lowcov 0.6 --pv_skip -m $VADRSCRIPTSDIR/testfiles/models/entoy100a.cm -i $VADRSCRIPTSDIR/testfiles/models/entoy100a.extrant.minfo $VADRSCRIPTSDIR/testfiles/entoy100a-extrant.fa va-entoy100a-extrant3 > va-entoy100a-extrant3.out
desc: annotate-entoy100a-extrant3
out: va-entoy100a-extrant3/va-entoy100a-extrant3.vadr.alt
